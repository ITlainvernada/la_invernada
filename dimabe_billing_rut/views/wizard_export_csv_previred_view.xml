<odoo>
    <data>
        <record id="wizard_export_csv_previred_view_inherit" model="ir.ui.view">
            <field name="name">wizard_export_csv_previred_view_inherit</field>
            <field name="model">wizard.export.csv.previred</field>
            <field name="inherit" ref="l10n_cl_hr.wizard_export_csv_previred_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='delimiter_option']" position="before">
                    <field name="month"/>
                    <field name="year"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
