<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="custom_parameter_tree" model="ir.ui.view">
            <field name="name">Tramos</field>
            <field name="model">custom.parameter</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="value"/>
                </tree>
            </field>
        </record>
        <record id="custome_parameter_action" model="ir.actions.act_window">
            <field name="name">custome_parameter.action</field>
            <field name="res_model">custome_parameter</field>
            <field name="view_type">form</field>
            <field name='view_id' ref="custom_parameter_tree"/>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem
            id="custom_parameter_menu"
            string="Tramos"
            parent=
            sequence="10" />
    </data>
</odoo>