<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="hr_contract_inherit" model="ir.ui.view">
            <field name="name">hr_contract_form</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='advantages']" position="after">
                    <field name="data_id"/>
                </xpath>
            </field>
        </record>
        <record id="action_private_rol_action" model="ir.actions.act_windows.view">
            <field name="act_window_id" ref="hr_contract.action_hr_contract	"/>
            <field name="view_id" ref=""
        </record>
    </data>
</odoo>