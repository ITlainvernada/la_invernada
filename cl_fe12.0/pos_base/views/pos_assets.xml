<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="assets_backend" name="pos_base assets" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">
        	<script type="text/javascript" src="/pos_base/static/lib/JsBarcode/JsBarcode.all.js"></script>
            <script type="text/javascript" src="/pos_base/static/src/js/popup.js"/>
            <script type="text/javascript" src="/pos_base/static/src/js/db.js"/>
            <script type="text/javascript" src="/pos_base/static/src/js/models.js"/>
            <script type="text/javascript" src="/pos_base/static/src/js/chrome.js"/>
            <script type="text/javascript" src="/pos_base/static/src/js/screens.js"/>
            <link rel="stylesheet" type="text/css" href="/web/static/lib/select2/select2.css"/>
            <link rel="stylesheet" type="text/css" href="/web/static/lib/jquery.ui/jquery-ui.css"/>
            <link rel="stylesheet" type="text/css" href="/pos_base/static/src/css/pos.css"/>
            <link rel="stylesheet" type="text/css" href="/pos_base/static/src/css/pos_customs.css"/>
        </xpath>
    </template>
    
    <template id="assets_export_backend" name="pos_base_export assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/pos_base/static/src/js/backup.js"></script>
        </xpath>
    </template>
</odoo>
