<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
	<record id="ir_cron_send_picking_no_done_on_date" model="ir.cron"
		forcecreate="True">
		<field name="name">Enviar Albaranes no procesados en fecha prevista</field>
		<field eval="True" name="active" />
		<field name="user_id" ref="base.user_root" />
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="doall" eval="False" />
		<field name="priority" eval="100" />
		<field name="model_id" ref="stock.model_stock_picking" />
		<field name="state">code</field>
		<field name="code">model.send_mail_picking_no_done_on_date()</field>
		<field name="nextcall"
			eval="(datetime.now() + relativedelta(hours=1, minute=0, second=0)).strftime('%Y-%m-%d %H:%M:%S')" />
	</record>

</odoo>