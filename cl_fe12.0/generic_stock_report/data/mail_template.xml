<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="et_picking_no_done_on_date" model="mail.template">
		<field name="name">Albaranes no procesados en fecha prevista</field>
		<field name="body_type">qweb</field>
		<field name="body_view_id" ref="view_et_picking_no_done_on_date" />
		<field name="model_id" ref="stock.model_stock_picking" />
		<field name="subject">Albaranes no procesados en fecha prevista</field>
		<field name="email_from">"${user.company_id.name|safe}" &lt;${user.email|safe}&gt;</field>
	</record>
	
	<record id="et_picking_partial" model="mail.template">
		<field name="name">Albaranes procesados parcialmente</field>
		<field name="body_type">qweb</field>
		<field name="body_view_id" ref="view_et_picking_partial" />
		<field name="model_id" ref="stock.model_stock_picking" />
		<field name="subject">Albaranes procesados parcialmente</field>
		<field name="email_from">"${user.company_id.name|safe}" &lt;${user.email|safe}&gt;</field>
	</record>
</odoo>
