		<field name="model">ir.sequence</field>
		<field name="inherit_id"
			ref="l10n_cl_fe.view_sequence_dte_caf_form" />
		<field name="arch" type="xml">
			<field name="forced_by_caf" position="after">
				<field name="show_in_pos"
					attrs="{'invisible': [('sii_document_class_id', '=', False)]}" />
				<field name="mode_online"
					attrs="{'invisible': [('sii_document_class_id', '=', False)]}" />
				<field name='correct_folios_automatic'
					attrs="{'invisible': [('sii_document_class_id', '=', False)]}" />
			</field>
		</field>
	</record>
</odoo>