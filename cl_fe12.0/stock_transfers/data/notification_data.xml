<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

	<record model="util.notification" id="not_transfer_new">
		<field name="name">Nueva Solicitud de Transferencia</field>
		<field name="header">Se ha solicitado transferir los siguientes productos
		</field>
	</record>

	<record model="util.notification" id="not_transfer_approved">
		<field name="name">Solicitud de Transferencia Aprobada</field>
		<field name="header">La solicitud de transferencia a sido aprobada y esta
			lista para transferir</field>
	</record>

	<record model="util.notification" id="not_transfer_rejected">
		<field name="name">Solicitud de Transferencia Rechazada</field>
		<field name="header">La Solicitud de transferencia ha sido rechazada
		</field>
	</record>

	<record model="util.notification" id="not_transfer_partial">
		<field name="name">Recepcion parcial de transferencia</field>
		<field name="header">La transferencia ha sido Recibida parcialmente
		</field>
	</record>
</odoo>