<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="stock_return_picking_form_view" model="ir.ui.view">
		<field name="name">stock.return.picking.form</field>
		<field name="model">stock.return.picking</field>
		<field name="inherit_id" ref="stock.view_stock_return_picking_form" />
		<field name="arch" type="xml">
			<field name="move_dest_exists" position="before">
				<group colspan="4">
					<field name="is_picking_out" invisible="1" />
					<field name="use_documents" attrs="{'invisible': [('is_picking_out','=',False)]}" />
				</group>
			</field>
		</field>
	</record>
</odoo>