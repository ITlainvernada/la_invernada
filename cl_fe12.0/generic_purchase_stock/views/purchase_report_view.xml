<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="purchase_report_search_view" model="ir.ui.view">
		<field name="name">purchase.report.search</field>
		<field name="model">purchase.report</field>
		<field name="inherit_id" ref="purchase.view_purchase_order_search" />
		<field name="arch" type="xml">
			<filter name="user_id" position="after">
				<filter name="group_by_picking_type" string="Destino"
					context="{'group_by': 'picking_type_id'}" groups="stock.group_stock_multi_locations" />
			</filter>
		</field>
	</record>
</odoo>