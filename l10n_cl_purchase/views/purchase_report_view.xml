<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<record id="purchase_report_graph_view" model="ir.ui.view">
		<field name="name">purchase.report.graph</field>
		<field name="model">purchase.report</field>
		<field name="inherit_id" ref="purchase.view_purchase_order_graph" />
		<field name="arch" type="xml">
			<field name="date_order" position="replace" />
			<field name="partner_id" position="replace">
				<field name="picking_type_id" type="row" />
				<field name="category_id" type="row" />
			</field>
		</field>
	</record>
	
	<record id="purchase_report_pivot_view" model="ir.ui.view">
		<field name="name">purchase.report.pivot</field>
		<field name="model">purchase.report</field>
		<field name="inherit_id" ref="purchase.view_purchase_order_pivot" />
		<field name="arch" type="xml">
			<field name="date_order" position="replace" />
			<field name="partner_id" position="replace">
				<field name="picking_type_id" type="row" />
				<field name="category_id" type="row" />
			</field>
		</field>
	</record>

</odoo>