<odoo>
    <data>
        <record id="view_mrp_dispatched_tree" model="ir.ui.view">
            <field name="name">Despachos</field>
            <field name="model">stock.picking</field>
            <field name="arch" type="xml">
                <tree string="Dispatched">
                    <field name="required_loading_date" />
                    <field name="partner_id" widget="many2one" />
                    <field name="product_id" widget="many2one"/>
                    <field name="variety" />
                    <field name="origin" />
                    <field name="state" />
                    <field name="name" />
                    <field name="shipping_number" />
                    <field name="client_label" />
                    <field name="charging_mode" />
                </tree>
            </field>
        </record>
        <record id="action_mrp_dispatched" model="ir.actions.act_window">
            <field name="name">Dispatched</field>
            <field name="res_model">stock.picking</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="menu_mrp_dispatched_root" name="Despachos" parent="mrp.menu_mrp_root" groups="base.group_user" />
        <menuitem id="mrp_dispatched_child" name="Despachos" parent="menu_mrp_dispatched_root" action="action_mrp_dispatched" />
    </data>
</odoo>