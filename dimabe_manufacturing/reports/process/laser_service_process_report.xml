<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="view_process_report_laser_service_tree" model="ir.ui.view">
            <field name="name">Informe de Proceso Partido Mecanico/Laser Servicio</field>
            <field name="model">stock.production.lot.serial</field>
            <field name="priority">20</field>
            <field name="arch" type="xml">
                <tree name="Informe de Proceso Partido Mecanico/Laser Servicio">
                    <field name="production_id_to_view" string="Orden de Fabricacion" widget="many2onebutton"/>
                    <field name="movement"/>
                    <field name="workcenter_id" widget="many2onebutton"/>
                    <field name="product_variety"/>
                    <field name="producer_id" widget="many2onebutton"/>
                    <field name="product_id" widget="many2onebutton"/>
                    <field name="serial_number"/>
                    <field name="stock_production_lot_id" widget="many2onebutton"/>
                    <field name="in_weight"/>
                    <field name="produce_weight"/>
                    <field name="pallet_id" widget="many2onebutton"/>
                    <field name="sale_order_id" widget="many2onebutton"/>
                </tree>
            </field>
        </record>
        <record id="dimabe_manufacturing.process_report_laser_service" model="ir.actions.act_window">
            <field name="name">Informe de Proceso Partido Mecanico/Laser Servicio</field>
            <field name="res_model">stock.production.lot.serial</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_process_report_laser_service_tree"/>
            <field name="domain">
                [('workcenter_id.code','=','870-PPMS')]
            </field>
        </record>
        <menuitem id="process_laser_menu"
                  action="dimabe_manufacturing.process_report_laser"
                  name="Informe de Proceso Partido Mecanico/Laser"
                  parent="process_report_menu"
        />

    </data>
</odoo>