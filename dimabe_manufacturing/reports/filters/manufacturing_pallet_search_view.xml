<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data>
    <record id="pallet_search_view" model="ir.ui.view">
      <field name="name">Pallet Search View</field>
      <field name="model">manufacturing.pallet</field>
      <field name="arch" type="xml">
        <field name="producer_id"/>
        <group expand="1" string="Group By">
          <filter string="Productor" name="producer_id" context="{'group_by':'producer_id'}"/>
        </group>
      </field>
    </record>
  </data>
</odoo>