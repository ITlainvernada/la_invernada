<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="stock_quant_search_view" model="ir.ui.view">
		<field name="name">stock.quant.search</field>
		<field name="model">stock.quant</field>
		<field name="inherit_id" ref="stock.quant_search_view" />
		<field name="arch" type="xml">
			<field name="location_id" position="after">
				<field name="in_date" widget="date" />
			</field>
		</field>
	</record>
	<record model="ir.actions.act_window" id="stock.quantsact">
		<field name="view_mode">pivot,tree,form</field>
		<field name="context">{'search_default_internal_loc': 1, 'search_default_filter_by_month': 1}</field>
	</record>
</odoo>