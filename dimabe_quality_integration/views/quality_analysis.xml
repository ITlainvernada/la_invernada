<odoo>
  <record id="quality_analysis_form_view" model="ir.ui.view">
    <field name="name">Formulario Analisis de Calidad</field>
    <field name="model">quality.analysis</field>
    <field name="arch" type="xml">
      <form delete="false" create="false" edit="false">
        <group>
          <group>
            <field name="pre_caliber"/>
            <field name="caliber_ids"/>
            <field name="external_damage_analysis_ids"/>
            <field name="color_analysis_ids"/>
          </group>
          <group>
            <field name="humidity_analysis_id"/>
            <field name="humidity_percent"/>
            <field name="humidity_tolerance"/>
            <field name="performance_analysis_ids"/>
            <field name="internal_damage_analysis_ids"/>
            <field name="analysis_observations"/>
            <field name="category"/>
          </group>
        </group>
      </form>
    </field>
  </record>

  <record id="quality_analysis_tree_view" model="ir.ui.view">
    <field name="name">Listado de Calidad</field>
    <field name="model">quality.analysis</field>
    <field name="arch" type="xml">
      <tree delete="false" edit="false" create="false">
        <field name="lot_name"/>
        <field name="pre_caliber"/>
        <field name="caliber_1" />
        <field name="caliber_2" />
        <field name="caliber_3" />
        <field name="caliber_4" />
        <field name="caliber_5" />
        <field name="caliber_6" />
        <field name="external_damage_analysis_1" />
        <field name="external_damage_analysis_2" />
        <field name="external_damage_analysis_3" />
        <field name="external_damage_analysis_4" />
        <field name="external_damage_analysis_5" />
        <field name="external_damage_analysis_6" />
        <field name="external_damage_analysis_7" />
        <field name="external_damage_analysis_8" />
        <field name="external_damage_analysis_9" />
        <field name="external_damage_analysis_10" />
        <field name="external_damage_analysis_11" />
        <field name="internal_damage_analysis_1" />
        <field name="internal_damage_analysis_2" />
        <field name="internal_damage_analysis_3" />
        <field name="internal_damage_analysis_4" />
        <field name="internal_damage_analysis_5" />
        <field name="internal_damage_analysis_6" />
        <field name="internal_damage_analysis_7" />
        <field name="humidity_percent" />
        <field name="humidity_tolerance" />
        <field name="performance_analysis_1" />
        <field name="performance_analysis_2" />
        <field name="color_analysis_ids" />
        <field name="form_analysis_ids" />
        <field name="impurity_analysis_ids" />
        <field name="analysis_observations" />
        <field name="category" />
      </tree>
    </field>
  </record>

  <record id="dimabe_quality_integration.quality_analysis_action" model="ir.actions.act_window">
    <field name="name">Listado de Calidad</field>
    <field name="res_model">quality.analysis</field>
    <field name="view_mode">tree,form</field>
  </record>

  <menuitem id="quality_analysis_tree_menu"
            action="dimabe_quality_integration.quality_analysis_action"
            name="Listado de Calidad"
            parent="mrp.mrp_planning_menu_root"
  />

</odoo>