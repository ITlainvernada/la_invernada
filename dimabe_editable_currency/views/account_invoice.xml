<odoo>
    <data>
        <record model="ir.ui.view" id="account_invoice_supplier_form_inherit">
            <field name="name">formulario factura de compra</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_supplier_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_line_ids']/tree//field[@name='invoice_line_tax_ids']" position="after">
                    <field name="exempt"/>
                 </xpath>
                 <xpath expr="//header" position="inside">
                    <button name="send_invoice"  string="Test Json"  type="object"/>
                 </xpath>
                <field name="partner_bank_id" position="after">
                    <field name="exchange_rate" />
                </field>
            </field>
        </record>

        <record model="ir.ui.view" id="account_invoice_form_inherit">
            <field name="name">formulario factura</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_form" />
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="send_invoice"  string="Test Json"  type="object"/>
                 </xpath>
                 <xpath expr="//field[@name='invoice_line_ids']/tree//field[@name='invoice_line_tax_ids']" position="after">
                    <field name="exempt"/>
                 </xpath>
                 
                <field name="currency_id" position="after">
                    <field name="exchange_rate" />
                </field>
            </field>
        </record>
       <record model="ir.ui.view" id="account_invoice_additional">
            <field name="name">Observaciones</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="dimabe_billing_rut.xdte_invoice_form_supplier"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='xdte_tab']" position="inside">
                <group string="Observaciones">
                    <field name="observations" mode="tree">
                        <form>
                            <field name="observations_ids"/>
                        </form>
                        <tree>
                            <field name="observations_ids"/>
                        </tree>
                    </field>
                    </group>
                </xpath>
            </field>
       </record>
    </data>
</odoo>