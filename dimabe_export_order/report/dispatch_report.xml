<odoo>
    <template id="report_dispatch">
        <t t-call="web.html_container">
            <t t-foreach="files" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2 t-field="o.datas_fname"></h2>
                    </div>
                </t>
            </t>
        </t>
    </template>
    <report
        id="action_dispatch_report"
        string="Informe de Despachos"
        model="ir.attachment"
        report_type="qweb-pdf"
        file="ir.attachment.local_url"
    />
</odoo>