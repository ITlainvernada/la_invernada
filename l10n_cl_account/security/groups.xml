<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
		<!-- grupo para ver menus relacionados con subir xml para crear facturas -->
		<record id="group_sii_upload_xml" model="res.groups" forcecreate="True">
			<field name="name">Subir XML DTE</field>
			<field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
			<field name="category_id" ref="generic_account.module_category_accounting_and_finance" />
		</record>
		
		<!-- grupo para ver la opcion de crear productos cuando no existen al importar xml -->
		<record id="group_create_product_xml" model="res.groups" forcecreate="True">
			<field name="name">Crear productos desde XML DTE</field>
			<field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
			<field name="category_id" ref="generic_account.module_category_accounting_and_finance" />
		</record>
	</data>
</odoo>